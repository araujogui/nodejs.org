{
  "extends": ["//"],
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": [],
      "outputs": ["dist/**"]
    },
    "lint:js": {
      "inputs": ["src/**/*.{js,mjs,ts,tsx}"]
    },
    "lint:md": {
      "inputs": ["src/**/*.md?(x)"],
      "outputs": [".eslintmdcache"]
    }
  }
}
